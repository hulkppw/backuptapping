{
  "name": "重启前的备份",
  "version": "1.0.2",
  "description": "Merges all of the browser's windows into the current window",
  "icons": {
    "48": "merge_windows_48.png",
    "128": "merge_windows_128.png"
  },
  "permissions":[
    "tabs",
    "bookmarks"
  ],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "arrow_in.png",
    "default_title": "重启前的备份"
  },
  "manifest_version": 2
}
